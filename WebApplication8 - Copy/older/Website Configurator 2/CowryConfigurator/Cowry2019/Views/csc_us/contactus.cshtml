
@{
    ViewBag.Title = "contactus";
    Layout = "~/Views/Shared/cscpartnerapp.cshtml";
}

<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular.min.js"></script>
<script src="~/JsControllers/csc_usCon.js"></script>


<div class="container-fluid" ng-app="cscContactUS" ng-controller="cscContactUSCtrl">
    <div class="row">
        <div class="col-md-12">
            <div class="card" style="background:#F2F4FA">
                <div class="card-header card-header-primary">
                    <h2 class="card-title">Contact Us</h2>
                    <p class="card-category">*All inputs are mandatory.</p>
                </div>
                <div class="card-body">
                    <form novalidate="" method="POST" id="myForm1">
                        <div class="row">
                            <div class="card" style="margin-bottom: 10px; margin-top: 5px;">
                                <!-- Card header -->
                                <div class="card-header">
                                    <h4 class="card-title">Personal Information</h4>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="firstname1" class="bmd-label-floating"> First Name* </label>
                                                <input type="text" id="firstname1" name="firstname1" class="form-control" aria-required="true" ng-model="myData.FirstName" required>
                                                <div class="invalid-feedback"><b>Please enter first name</b></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="lastName1" class="bmd-label-floating">Last Name*</label>
                                                <input type="text" id="lastName1" name="lastName1" class="form-control" aria-required="true" ng-model="myData.LastName" required>
                                                <div class="invalid-feedback"><b>Please enter last name</b></div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="jobTitle1" class="bmd-label-floating">Job Title*</label>
                                                <input type="text" id="jobTitle1" name="jobTitle1" class="form-control" aria-required="true" ng-model="myData.JobTitle" required>
                                                <div class="invalid-feedback"><b>Please enter job title</b></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="contactNo1" class="bmd-label-floating">Contact Number*</label>
                                                <input type="text" id="contactNo1" name="contactNo1" class="form-control" aria-required="true" ng-model="myData.ContactNumber" required>
                                                <div class="invalid-feedback"><b>Please enter contact number</b></div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="email1" class="bmd-label-floating">Email Address*</label>
                                                <input type="text" class="form-control" id="email1" name="email1" name="email" email="true" aria-required="true" ng-model="myData.Email" required>
                                                <div class="invalid-feedback"><b>Please enter email address</b></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="confirmemail1" class="bmd-label-floating">Confirm Email Address*</label>
                                                <input type="text" class="form-control" id="confirmemail1" name="confirmemail1" aria-required="true" ng-model="myData.ConfirmEmail" required>
                                                <div class="invalid-feedback"><b>Please enter confirm email address</b></div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="card" style="margin-bottom: 10px; margin-top: 5px;">
                                <div class="card-header">
                                    <h4 class="card-title">Company Information</h4>
                                </div>
                                <div class="card-body">

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="companyName1" class="bmd-label-floating">Company Name*</label>
                                                <input type="text" id="companyName1" name="companyName1" class="form-control" ng-model="myData.CompanyName" required>
                                                <div class="invalid-feedback"><b>Please enter company name.</b></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="phoneNumber1" class="bmd-label-floating">Phone Number*</label>
                                                <input type="text" id="phoneNumber1" name="phoneNumber1" class="form-control" ng-model="myData.CompanyNumber" required>
                                                <div class="invalid-feedback"><b>Please enter phone number.</b></div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group bmd-form-group" style="margin-right:15px;margin-left:15px;">
                                        <label for="address1" class="bmd-label-floating">Address*</label>
                                        <input type="text" id="address1" name="address1" class="form-control" ng-model="myData.CompanyAddress" required>
                                        <div class="invalid-feedback"><b>Please enter company address.</b></div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="contries1" class="bmd-label-floating">Country*</label>
                                                <select class="form-control" id="contries1" name="contries1" ng-model="myData.CompanyCountry" required>
                                                    <option ng-repeat="temp in allCountries">{{temp.name}}</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="state1" class="bmd-label-floating">State Province*</label>
                                                <input type="text" id="state1" name="state1" class="form-control" ng-model="myData.CompanyState" required>
                                                <div class="invalid-feedback"><b>Please enter state.</b></div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="city1" class="bmd-label-floating">City*</label>
                                                <input type="text" id="city1" name="city1" class="form-control" ng-model="myData.CompanyCity" required>
                                                <div class="invalid-feedback"><b>Please enter city.</b></div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group bmd-form-group">
                                                <label for="postalcode1" class="bmd-label-floating">Postal Code*</label>
                                                <input type="text" id="postalcode1" name="postalcode1" class="form-control" ng-model="myData.CompanyPostalCode" required>
                                                <div class="invalid-feedback"><b>Please enter postal code.</b></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="pull-right" style="margin: 10px;">
                    <div class="row">
                        <div class="col-md-6">
                            <h5 class="pull-left" style="border-radius:5px;">
                                <button class="btn btn-warning" style="font-size:75%" ; onclick="ShowHome()">
                                    <span class="btn-label btn-label-right">
                                        <i class="material-icons">keyboard_arrow_left</i>
                                    </span>
                                    Back to Home
                                </button>
                            </h5>
                        </div>
                        <div class="col-md-6">
                            <h5 class="pull-right" style="border-radius:5px;">
                                <button class="btn btn-success" style="font-size:75%" ng-click="checkContactData(myData)">
                                    Submit
                                </button>
                            </h5>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<script>
    function ShowHome() {
        window.location.href = "/us/home_us";
    }
</script>

