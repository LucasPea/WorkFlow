<script>
    var currentTab = 0;
    var currentConf = 0;
    var subTotal = 0;
    showTab(currentTab, 0);

    function showTab(n, m) {
        var x = document.getElementsByClassName("tab");
        x[n].style.display = "block";
        if (n == 0) {                                                               //start
            document.getElementById("DivContinue").style.display = "none";
            document.getElementById("prevBtn").style.display = "none";
            document.getElementById("nextBtn").style.display = "none";
            document.getElementById("btn_nextNav").style.display = "none";
            document.getElementById("txt_Welcome").style.display = "";
            document.getElementById("LightningLogo").style.display = "none";
            document.getElementById("btn_startConf").style.display = "";
            document.getElementById("navDot").style.display = "none";
            document.getElementById("stepStart").style.visibility = "hidden";
            document.getElementById("step1").style.visibility = "hidden";
            document.getElementById("OneTimeDiv").style.display = "none";
            navFix(n);
        } else {
            if (n == m) {
                document.getElementById("NavDiv").style.display = "none";
            } else {
                document.getElementById("stepN4").removeAttribute("hidden");
                navFix(currentConf);
                document.getElementById("txt_Welcome").style.display = "none";
                document.getElementById("LightningLogo").style.display = "";
                document.getElementById("DivContinue").style.display = "";
                if (currentConf != 0 && currentConf != 2 && currentConf != 3) {
                    document.getElementById("prevBtn").style.display = "inline";
                    document.getElementById("nextBtn").style.display = "inline";
                    document.getElementById("navDot").style.display = "";
                    document.getElementById("OneTimeDiv").style.display = "flex";
                }
                else {
                    document.getElementById("navDot").style.display = "none";
                    document.getElementById("prevBtn").style.display = "none";
                    document.getElementById("nextBtn").style.display = "none";
                    document.getElementById("OneTimeDiv").style.display = "none";

                }
                if (currentConf == 2 || currentConf == 3) {
                    document.getElementById("btn_nextNav").style.display = "none";
                } else {
                    document.getElementById("btn_nextNav").style.display = "";
                }
                document.getElementById("btn_startConf").style.display = "none";
            }
        }
        fixStepIndicator(n)
        topFunction();
    }

    function checkActive(n) {
        var test = document.getElementById(n).className;
        var substring = " active";
        if (test.includes(substring)) {
            return true;
        } else {
            return false;
        }
    }

    function navFix(n) {
        currentConf = n;
        var step3 = document.getElementById('stepN4');
        var btn3 = document.getElementById('btnPricing');
        var step3t = document.getElementsByTagName("h7");
        for (var text = 0; text < step3t.length; text++) {
            step3t[text].className = step3t[text].className.replace("h7 active", "");
        }
        step3.className = step3.className.replace(" active", "");
        step3.style.backgroundColor = "White";
        btn3.className = btn3.className.replace(" transparent2", "");
        if (currentConf == 3) {
            for (var text = 0; text < step3t.length; text++) {
                step3t[text].className += "h7 active";
            }
            step3.className += " active";
            step3.style.backgroundColor = "#7389c0";
            btn3.className += " transparent2";
        }
        for (var aux = 0; aux <= 2; aux++) {
            var doc = document.getElementById('stepN' + (aux + 1));
            doc.style.backgroundColor = "White";
            //var src = "~/Content/assets/img/NavBar" + aux + ".png";
            //document.getElementById('img' + aux).src = document.getElementById('img' + aux).src.replace('Active.png', '.png');
            if (aux == currentConf && currentTab != 0) {
                //document.getElementById('img' + aux).src = document.getElementById('img' + aux).src.replace('.png', 'Active.png');
                doc.style.backgroundColor = "#7389c0";
            }
        }
    }

    function validateForm() {
        var x, y, i, valid = true;
        x = document.getElementsByClassName("tab");
        y = x[currentTab].getElementsByTagName("input");
        for (i = 0; i < y.length; i++) {
            if (y[i].value == "") {
                y[i].className += " invalid";
            }
        }
        return valid;
    }

    function fixStepIndicator(n) {
        var i, x = document.getElementsByClassName("step");
        for (i = 0; i < x.length; i++) {
            x[i].className = x[i].className.replace(" finish", "");
            if (i == n || n == 0) {
                x[i].className = x[i].className.replace(" finish", "");//finish
            }
        }
        x[n].className += " finish";
    }

    function topFunction() {
        $('html,body').animate({ scrollTop: 91 }, 'slow');
    }

    function setPaymentOption() {
        var month = document.getElementById("td_serviceTotal").innerHTML;
        var annuity = document.getElementById("td_packageOneTimeTotal").innerHTML;

        if (!isInt2(month)) {
            document.getElementById("standarTermsPayMonth").innerHTML = parseFloat(month).toFixed(2);
            document.getElementById("txt_Month").innerHTML = parseFloat(month).toFixed(2);
            document.getElementById("txt_oneMonth").innerHTML = parseFloat(month).toFixed(2);
        } else {
            document.getElementById("standarTermsPayMonth").innerHTML = parseInt(month);
            document.getElementById("txt_Month").innerHTML = parseInt(month);
            document.getElementById("txt_oneMonth").innerHTML = parseInt(month);
        }

        if (isInt2(annuity)) {
            document.getElementById("standarTermsPayOneTime").innerHTML = parseInt(annuity);
        } else {
            document.getElementById("standarTermsPayOneTime").innerHTML = parseFloat(annuity).toFixed(2);
        }

        var totalAux = (parseFloat(annuity) * 1.1) / 12;
        totalAux += parseFloat(month);
        if (isInt2(totalAux)) {
            document.getElementById("txt_standarTermsPayMonth").innerHTML = parseInt(totalAux);
        } else {
            document.getElementById("txt_standarTermsPayMonth").innerHTML = parseFloat(totalAux).toFixed(2);
        }

        totalAux = parseFloat(annuity) * 0.9;
        totalAux += (parseFloat(month) * 12);
        //totalAux = Math.round(totalAux);
        if (isInt2(totalAux)) {
            document.getElementById("txt_standarTermsPayOneTime").innerHTML = parseInt(totalAux);
        } else {
            document.getElementById("txt_standarTermsPayOneTime").innerHTML = parseFloat(totalAux).toFixed(2);
        }


    }

    function isInt2(n) {
        return n % 1 === 0;
    }
    function backhome() {
        location.href = 'https://cowrysolutions.com';
    }

    function changeGetAddress() {
        var country = document.getElementById('countryId').value;
        document.getElementById('invalidZIP').setAttribute("hidden", true);

        var zip = document.getElementById('postalcode').value;
        if (country != "") {
            if (zip != "") {
                getAddress();
            }
        }
    }

    function getAddress() {
        var country = document.getElementById('countryId').value;
        if (country != "") {
            document.getElementById('invalidZIP').setAttribute("hidden", true);
            document.getElementById('spinnerVal').removeAttribute("hidden");
            var zip = document.getElementById('postalcode').value;
            callAddress("https://secure.geonames.org/postalCodeSearch?postalcode=" + zip + "&country=" + country + "&maxRows=1&username=remotingcoders");
        } else {
            document.getElementById('invalidZIP').setAttribute("hidden", true);
        }
    }
    function callAddress(url) {
        const http = new XMLHttpRequest();
        http.open("GET", url);
        http.send();
        http.onload = function () {
            document.getElementById('spinnerVal').setAttribute("hidden", true);
            var data = http.responseText;

            var parser = new DOMParser();
            var xmlDoc = parser.parseFromString(data, 'text/xml');

            if (xmlDoc.getElementsByTagName("totalResultsCount")[0].childNodes[0].nodeValue != 0) {
                if (xmlDoc.getElementsByTagName("countryCode")[0].childNodes[0].nodeValue == "GB") {
                    document.getElementById('stateTest').value = xmlDoc.getElementsByTagName("adminName2")[0].childNodes[0].nodeValue;
                } else {
                    document.getElementById('stateTest').value = xmlDoc.getElementsByTagName("adminCode1")[0].childNodes[0].nodeValue;
                }
                document.getElementById('cityTest').value = xmlDoc.getElementsByTagName("name")[0].childNodes[0].nodeValue;
                console.log(document.getElementById('cityTest').value);
                document.getElementById('invalidZIP').setAttribute("hidden", true);
            } else {
                document.getElementById('cityTest').value = "";
                document.getElementById('stateTest').value = "";

                document.getElementById('invalidZIP').removeAttribute("hidden");
            }
        }
    }

    function btnpdf_Click(th) {
        window.open(th.name, '_blank');
    }

</script>
